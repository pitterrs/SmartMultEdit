<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:sf="sap.ui.comp.smartform"
		xmlns:smartField="sap.ui.comp.smartfield"
		xmlns:core="sap.ui.core"
		xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
		xmlns:smartmultiedit="sap.ui.comp.smartmultiedit">
	<Dialog
			title="Edit Multiple"
			class="sapUiContentPadding"
			stretchOnPhone="true"
			id="multipleEditingDialog"
			contentWidth="40rem"
			horizontalScrolling="false">
		<content>
			<smartmultiedit:Container entitySet="zce_prorroga_vencimento" id="multiEditContainer"
									  app:defaultDropDownDisplayBehaviour="descriptionOnly">
				<smartmultiedit:customData>
					<core:CustomData key="dateFormatSettings" value='\{"UTC":true,"style":"long"\}'/>
				</smartmultiedit:customData>
				<smartmultiedit:layout>
					<sf:SmartForm editable="true" >
						<sf:layout>
							<sf:Layout labelSpanM="12" labelSpanL="12" labelSpanXL="12" />
						</sf:layout>
						<sf:groups>
							<sf:Group>
								<!-- <sf:GroupElement>
									<sf:elements>
										<smartmultiedit:Field propertyName="BELNR" useApplyToEmptyOnly="true"/>
									</sf:elements>
								</sf:GroupElement>
								<sf:GroupElement>
									<sf:elements>
										<smartmultiedit:Field propertyName="GJAHR"/>
									</sf:elements>
								</sf:GroupElement> -->
								<sf:GroupElement>
									<sf:elements>
										<smartmultiedit:Field propertyName="VLR_NF" useApplyToEmptyOnly="true"/>
									</sf:elements>
								</sf:GroupElement>
							</sf:Group>
						</sf:groups>
					</sf:SmartForm>
				</smartmultiedit:layout>
			</smartmultiedit:Container>
		</content>
		<buttons>
			<Button text="Save" press="onDialogSaveButton" type="Emphasized"/>
			<Button text="Cancel" press="onCloseDialog"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>

